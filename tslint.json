{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "linterOptions": {
    "exclude": [
      "src/**/__mocks__/*",
      "src/**/*.spec.ts"
    ]
  },
  "rules": {
    "no-var-requires": false,
    "import-name": false,
    "no-namespace": false
  }
}
